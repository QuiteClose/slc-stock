# Alpha Vantage — free key at https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=

# Polygon.io — free key at https://polygon.io/dashboard/signup
POLYGON_API_KEY=

# Default provider used by the API on cache miss (yfinance | alpha_vantage | polygon)
DEFAULT_PROVIDER=yfinance

# Years of history to fetch during background prefetch
PREFETCH_YEARS=3

# SQLite database path (relative to instance/)
DATABASE_URL=sqlite:///instance/quotes.db
